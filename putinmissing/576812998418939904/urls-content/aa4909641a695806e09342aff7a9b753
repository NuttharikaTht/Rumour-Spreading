<script async src="//cdn.taboola.com/libtrc/dailymail-uk/loader.js"></script>
<script type="text/javascript">
    DM.later('bundle', function(){
        var n = 0;
        var taboola_every_n = 0;
        var taboola_counter = taboola_every_n - 1;
        var placements = ["Stream Thumbnails 1st", "Stream Thumbnails 2nd"];

        var addTaboola = function (node){
            var id = "taboola-mobile-stream-thumbnails-" + (++n);
            var taboola_node = '<div id="' + id + '"></div>';
            $(node).after(taboola_node);

            window._taboola = window._taboola || [];
            _taboola.push({
                mode: 'thumbnails-b',
                container: id,
                placement: placements[(n-1) % 2],
                target_type: 'mix' });
        };

        DM.has('infinite-list', 'InfiniteList', {
url: '/api/infinite-list.html?channelShortName=news&pageSize=10',
            total: 15.0,
            from: 0,
            onAfterAppend: function (container){
                var items = $(container).children('div');
                if(!taboola_every_n){
                    return;
                }

                items.each(function (){
                    if (taboola_counter == 0){
                        addTaboola(this);
                        taboola_counter = taboola_every_n - 1;
                    }
                    else {
                        taboola_counter--;
                    }
                });
            }
        });
    });
</script>

