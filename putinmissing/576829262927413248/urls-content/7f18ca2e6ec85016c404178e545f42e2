                         <div id="audio_menu" class='multimedia_menu '> <div class="wrapper"> <div id="content_services_menuaudio1" class="column first_column">  </div> <div id="content_services_liveaudio1" class="column first_column" style="display: none;"> </div> <div id="content_services_menuaudio2" class="column second_column">  </div> <div id="content_services_liveaudio2" class="column second_column" style="display: none;"> </div> <div class="column third_column">  </div> <div class="delimiter"> <hr class="delimiter"/> </div> </div> </div>  <div id="itv_menu" class="multimedia_menu"> <div class="wrapper"> <div id="content_services_menuitv1" class="column first_column">  </div> <div id="content_services_liveitv1" class="column first_column" style="display: none;"> </div> <div id="content_services_menuitv2" class="column second_column">  </div> <div id="content_services_liveitv2" class="column second_column" style="display: none;"> </div> <div id="content_services_menuitv3" class="column third_column">  </div> <div class="delimiter"> <hr class="delimiter"/> </div> </div> </div> <script type="text/javascript">
    //<![CDATA[
    var audio_menu_num_of_opens = 0;
    var audio_menu_num_of_titleupdates = 0;
    var audio_menu_is_live = false;
    var audio_menu_liveid = '0';
    var itv_menu_num_of_opens = 0;
    var itv_menu_num_of_titleupdates = 0;
    var itv_menu_is_live = false;
    var itv_menu_liveid = '0';

    /*menu type is audio or itv*/
    function menu_open(openmenutype) {
        var audioAlwaysOpen = false ;
        var closemenutype = openmenutype == 'audio' ? 'itv' : 'audio'; 
        if ($('#'+openmenutype+'_menu').is(':visible')==false)
        {
            if ($('#'+closemenutype+'_menu').is(':visible'))
            {
                if(closemenutype!='audio' || !audioAlwaysOpen)
                {
                    $('#' + closemenutype + '_menu').slideToggle('fast');
                    $('#' + closemenutype + '_menu_trigger').toggleClass('opened');
                }
            }
            if(openmenutype!='audio' || !audioAlwaysOpen)
            {
                $('#' + openmenutype + '_menu').slideToggle('fast');
                $('#' + openmenutype + '_menu_trigger').toggleClass('opened');
            }
        }
    }
    function audio_menu_update_title(islive) {
        $('#audio_menu_trigger').html('<strong>'+
            (islive ?'Live Audio menu'
                :'Audio menu') +'</strong>');
        return false;
    }
    Sys.Application.add_load(function()
    {
        var showAudio = false;
        var showItv = false;
        var internetTvOpenHasPriority = true;
        var autoOpenEnabled = true;
        var autoRefreshInterval = 0;
        if((showAudio || showItv) ) //not in cms preview
        {
            www.rferl.org.Services.AudioHighlightServiceHelper.register("UpdateAudioHighlightMenu", autoRefreshInterval*1000, null, 
                function successCallbackAudio_AudioHighlightMenu(result){
                    var oA = $("#audio_menu");
                    oA.removeClass("multi_tube_menu");
                    //filling proper columns with data from service
                    if(result!=null)
                    {
                  //      if(result.HtmlAudioLive!='')
                    //        $("#content_services_liveaudio1").html(result.HtmlAudioLive);
                      //  if(result.HtmlAudioUpcomming!='')
                        //    $("#content_services_liveaudio2").html(result.HtmlAudioUpcomming);
                        if (result.HtmlAudioMultiTubes) 
                        {
                            oA.addClass("multi_tube_menu");
                            $(".wrapper", oA).html(result.HtmlAudioMultiTubes); //replace whole menu with multi tubes custom menu
                        } 
                        else
                        {
                            if(result.HtmlAudioLive!='') //if live audio exist
                            {
                                $("#content_services_liveaudio1").html(result.HtmlAudioLive);
                                $("#content_services_menuaudio1").hide();
                                $("#content_services_liveaudio1").show();
                                if(result.HtmlAudioUpcomming!='') {
                                    $("#content_services_liveaudio2").html(result.HtmlAudioUpcomming);    
                                    $("#content_services_menuaudio2").hide();
                                    $("#content_services_liveaudio2").show();
                                }
                                else {
                                    $("#content_services_liveaudio2").hide();
                                    $("#content_services_menuaudio2").show();
                                }
                            } 
                            else //if live audio doesnt exist (second column display menu)
                            {
                                $("#content_services_liveaudio2").hide();
                                $("#content_services_menuaudio2").show();
                            
                                if(result.HtmlAudioUpcomming!='') { //if upcommming exist first column is upcomming and second column display menu
                                    $("#content_services_liveaudio1").html(result.HtmlAudioUpcomming);
                                    $("#content_services_menuaudio1").hide();
                                    $("#content_services_liveaudio1").show();
                                }
                                else {  // if no audio exist first column display menu too
                                    $("#content_services_liveaudio1").hide();
                                    $("#content_services_menuaudio1").show();
                                }
                            }
                        
                        
                            if(result.HtmlItvLive!='') //if live itv exist
                            {
                                $("#content_services_liveitv1").html(result.HtmlItvLive);
                                $("#content_services_menuitv1").hide();
                                $("#content_services_liveitv1").show();
                                if(result.HtmlItvUpcomming!='') {
                                    $("#content_services_liveitv2").html(result.HtmlItvUpcomming);    
                                    $("#content_services_menuitv2").hide();
                                    $("#content_services_liveitv2").show();
                                }
                                else {
                                    $("#content_services_liveitv2").hide();
                                    $("#content_services_menuitv2").show();
                                }
                            }
                            else //if live itv doesnt exist (second column display menu)
                            {
                                $("#content_services_liveitv2").hide();
                                $("#content_services_menuitv2").show();
                            
                                if(result.HtmlItvUpcomming!='') { //if upcommming exist first column is upcomming and second column display menu
                                    $("#content_services_liveitv1").html(result.HtmlItvUpcomming);
                                    $("#content_services_menuitv1").hide();
                                    $("#content_services_liveitv1").show();
                                }
                                else {  // if no itv exist first column display menu too
                                    $("#content_services_liveitv1").hide();
                                    $("#content_services_menuitv1").show();
                                }
                            }
                            audio_menu_liveid = result.AudioLiveId;    
                            itv_menu_liveid = result.ItvLiveId;    
                        }
                    }
                    
                    var oldvalue_audioislive = audio_menu_is_live;
                    audio_menu_is_live = audio_menu_liveid!='0';
                    itv_menu_is_live = itv_menu_liveid!='0';
                    
                    //check which audio was closed by user .. check in cookies
                    var lastclosed_audioid = $.cookie("audio_menu_liveid");
                    if(lastclosed_audioid==null)
                        lastclosed_audioid='0';

                    var lastclosed_itvid = $.cookie("itv_menu_liveid");
                    if(lastclosed_itvid==null)
                        lastclosed_itvid='0';

                    //logic which tab to automatically open
                    var openAudio = audio_menu_liveid != lastclosed_audioid && audio_menu_num_of_opens == 0 && audio_menu_is_live;
                    var openItv = itv_menu_liveid != lastclosed_itvid && itv_menu_num_of_opens == 0 && itv_menu_is_live;
                    if(autoOpenEnabled && (openAudio || openItv))
                    {
                        if(openItv && openAudio) {
                            openAudio = !internetTvOpenHasPriority;
                            openItv = internetTvOpenHasPriority;
                        }
                        if(openAudio) {
                            menu_open('audio');
                            audio_menu_num_of_opens = 1;
                        }
                        if(openItv) {
                            menu_open('itv');
                            itv_menu_num_of_opens = 1;
                        }
                        setOpenedMultimediaControl();
                    }
                    
                    if(audio_menu_num_of_titleupdates==0 || oldvalue_audioislive!=audio_menu_is_live)
                    {
                        audio_menu_update_title(audio_menu_is_live);
                        audio_menu_num_of_titleupdates=1;
                    }
                },
                function failCallbackAudio_AudioHighlightMenu(error){
                    // Hide the audio highlight on any error.
                    $("#content_services_liveaudio1").hide();
                    $("#content_services_liveaudio2").hide();
                    $("#content_services_menuaudio1").show();
                    $("#content_services_menuaudio2").show();
                    $("#content_services_liveaudio1").html("");
                    $("#content_services_liveaudio2").html("");
                    $("#content_services_liveitv1").hide();
                    $("#content_services_liveitv2").hide();
                    $("#content_services_menuitv1").show();
                    $("#content_services_menuitv2").show();
                    $("#content_services_liveitv1").html("");
                    $("#content_services_liveitv2").html("");
                }
            );
        }
    });
    //]]>
