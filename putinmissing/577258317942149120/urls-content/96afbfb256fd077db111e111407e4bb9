<script>
    $(document).ready(function () {
        // run the submit function, pin an event to it
        $('#header-search-form').submit(function (e) {
            // find the #s, which is the search input id
            var s = $(this).find("#s");

            // if s has no value, proceed
            if(!s.val()) {
                e.preventDefault();

                // alert that the search is empty
                alert("Your search is empty!");

                // focus on the search input
                $('#s').focus();
            }
        });
    });
</script>
